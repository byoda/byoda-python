{
    "description": "the private service for which no data will be shared with services or their members",
    "jsonschema": {
        "$id": "https://uuid.services.byoda.net/service/private",
        "$schema": "https://json-schema.org/draft-07/schema#",
        "description": "This is the default service of each network, storing only private data",
        "properties": {
            "member": {
                "#accesscontrol": [
                    "read"
                ],
                "properties": {
                    "joined": {
                        "description": "Date & time when the pod became a member of the service",
                        "format": "date-time",
                        "type": "string"
                    },
                    "member_id": {
                        "description": "Membership UUID",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "person": {
                "#accesscontrol": [
                    {
                        "member": [
                            "read",
                            "update",
                            "delete"
                        ]
                    }
                ],
                "properties": {
                    "additional_names": {
                        "access": "private",
                        "description": "Any middle names",
                        "type": "string"
                    },
                    "avatar_url": {
                        "access": "private",
                        "type": "string"
                    },
                    "email": {
                        "access": "private",
                        "format": "idn-email",
                        "type": "string"
                    },
                    "family_name": {
                        "access": "private",
                        "description": "Your surname",
                        "type": "string"
                    },
                    "given_name": {
                        "access": "private",
                        "description": "Your first given name",
                        "type": "string"
                    },
                    "homepage_url": {
                        "access": "private",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        },
        "title": "private service storing information about you",
        "type": "object"
    },
    "name": "private",
    "owner": "Steven Hessing",
    "service_id": 0,
    "signatures": {
        "network": {
            "certificate": "network.network-data.byoda.net",
            "hash_algorithm": "SHA256",
            "signature": "",
            "timestamp": "2021-11-18T03:41:54"
        },
        "service": {
            "certificate": "data.service-data-0.byoda.net",
            "hash_algorithm": "SHA256",
            "signature": "",
            "timestamp": "2021-11-18T03:41:54"
        }
    },
    "supportemail": "steven@byoda.org",
    "version": 1,
    "website": "https://www.byoda.org/"
}