{
    "description": "the private service for which no data will be shared with services or their members",
    "jsonschema": {
        "$id": "https://uuid.services.byoda.net/service/private",
        "$schema": "https://json-schema.org/draft-07/schema#",
        "description": "This is the default service of each network, storing only private data",
        "properties": {
            "member": {
                "#accesscontrol": [
                    {
                        "member": ["read"]
                    }
                ],
                "properties": {
                    "joined": {
                        "description": "Date & time when the pod became a member of the service",
                        "format": "date-time",
                        "type": "string"
                    },
                    "memberId": {
                        "description": "Membership UUID",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "memberlogs": {
                "#accesscontrol": [
                    {
                        "member": ["read", "delete", "append"]
                    }

                ],
                "type": "array",
                "items": [
                    {
                        "type": "object",
                        "properties": {
                            "timestamp": {
                                "description": "time the log entry was created",
                                "format": "data-time",
                                "type": "string"
                            },
                            "remote_addr": {
                                "description": "The remote IP address performing the request",
                                "format": "ipaddress",
                                "type": "string"

                            },
                            "action": {
                                "description": "What action was requested by the client",
                                "type": "string"
                            },
                            "message": {
                                "description": "Additional information about the request",
                                "type": "string"
                            }
                        }
                    }
                ]
            },
            "person": {
                "#accesscontrol": [
                    {
                        "member": [
                            "read", "update", "delete"
                        ]
                    },
                    {
                        "service": [
                            "read"
                        ],
                        "network": [
                            "read"
                        ]
                    }
                ],
                "properties": {
                    "additionalNames": {
                        "description": "Any middle names",
                        "type": "string"
                    },
                    "avatarUrl": {
                        "type": "string"
                    },
                    "email": {
                        "format": "idn-email",
                        "type": "string",
                        "#accesscontrol": [
                            {
                                "service": [
                                    "search:exact-caseinsensitive"
                                ]
                            }
                        ]
                    },
                    "familyName": {
                        "description": "Your surname",
                        "type": "string"
                    },
                    "givenName": {
                        "description": "Your first given name",
                        "type": "string"
                    },
                    "homepageUrl": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        },
        "title": "private service storing information about you",
        "type": "object"
    },
    "name": "private",
    "owner": "Steven Hessing",
    "service_id": 12345678,
    "signatures": {},
    "supportemail": "steven@byoda.org",
    "version": 1,
    "website": "https://www.byoda.org/"
}